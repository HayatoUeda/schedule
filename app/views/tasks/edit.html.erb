<h2>スケジュール編集</h2>
<%= form_for @task do |f| %>
    <%= render 'form', task: @task %>
        <div>
            <ul>
                <li><%= f.submit "編集を完了する"%></li>
                <li><%= link_to '確認', @task %></li>
                <li><%= link_to 'スケジュール一覧に戻る', tasks_path %></li>
            </ul>
        </div>
    <%end%>